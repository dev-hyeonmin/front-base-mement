{
  "name": "mement-frontend",
  "version": "1.0.0",
  "description": "This is a practice process for the base of the frontend to be used in the project.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-hyeonmin/front-base-mement"
  },
  "author": "roh._.hm",
  "license": "MIT",
  "workspaces": [
    "./packages/*",
    "./apps/*"
  ],
  "scripts": {
    "start:homepage": "npm run dev --workspace @mement-frontend/homepage",
    "start:admin": "npm run dev --workspace @mement-frontend/admin",
    "start:storybook": "npm run storybook --workspace @mement-frontend/common",
    "build:admin": "npm run build --workspace @mement-frontend/admin",
    "preview:admin": "npm run preview --workspace @mement-frontend/admin",
    "watch:styles": "npm run build --workspace @mement-frontend/styles",
    "build:styles": "npm run build --workspace @mement-frontend/styles"
  },
  "dependencies": {
    "vite-plugin-dts": "^3.6.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
